each site, index in sites
    tr
        each r,index in site.sites
            td(class=r.score < 50 ? "slow" : "",data-screenshot=r.filename)
                span.psi= r.score
                span.dcl= dcl(r.dcl)
        td
            span(title=moment(site.date).format('LLLL'))
            span.date(data-date=site.isoDate)=site.isoDate
