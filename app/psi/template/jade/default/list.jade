extends base
block content
    h1 Speed Monitoring for some publishers

    div.screenshot
    div.ct-chart
    table.table.table-striped.table-hover
        thead
            tr
                each site in sites[0].sites
                    th=site.label
                th
        tbody
            include row

    include ../common/more

    if !sites.length
        p No entries found.

    include ../common/footer


